{
  "name": "n8n-nodes-putio",
  "version": "0.1.5",
  "description": "n8n node for Put.io API integration",
  "keywords": [
    "n8n-community-node-package",
    "putio",
    "put.io",
    "file storage",
    "torrent"
  ],
  "license": "MIT",
  "homepage": "https://github.com/webthingee/n8n-nodes-putio",
  "author": {
    "name": "Sean Lange",
    "email": "sean@seanlange.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webthingee/n8n-nodes-putio.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint --config .eslintrc.js 'nodes/**/*.ts' 'credentials/**/*.ts' package.json",
    "lintfix": "eslint --config .eslintrc.js 'nodes/**/*.ts' 'credentials/**/*.ts' package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "icons"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PutioApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Putio/Putio.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "~0.125.0",
    "n8n-workflow": "~0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  }
}
